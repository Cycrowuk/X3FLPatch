<script>
<name>!wing.cmd.protect.pl</name>
<version>1</version>
<engineversion>63</engineversion>
<description>Wing Preload COMMAND PROTECT</description>
<arguments>
<argument index="1" name="wing" type="Var/Wing" desc="wing object" />
<argument index="2" name="a.event" type="Value" desc="Event" />
<argument index="3" name="a.data" type="Value" desc="Data" />
</arguments>
<sourceplaintext>
<![CDATA[
  * 18-03-08 (SymTec)
  $page.id = 6020
  
  if $a.event == [MEvent.Start]
    gosub sub.BuildMenu
    return $input
  else if $a.event == 'select'
    gosub sub.GetTarget
    return $input
  else if $a.event == 'select.target'
    if $a.data -> exists
      set global variable: name='menu.target' value=$a.data
      return [TRUE]
    end
  else if $a.event == 'select.menu'
    if $a.data == 'select'
      gosub sub.GetTarget
      return $input
    else if $a.data -> exists
      set global variable: name='menu.target' value=$a.data
      return [TRUE]
    else if is datatype[ $a.data ] == DATATYPE_ARRAY
      $target = $a.data[0]
      if $target -> exists
        set global variable: name='menu.target' value=$target
        return [TRUE]
      end
    end
  else if $a.event == [MEvent.Returned]
    $menu.id = $a.data[0]
    $selection = $a.data[1]
    $value = $a.data[2]
    if $menu.id == 'select.menu'
      if $selection == 'select.target'
        if $value -> exists
          $input = create menu return to previous menu: value=$value
          return $input
        end
      end
    end
  else if $a.event == [MEvent.Arguments]
    $target = get global variable: name='menu.target'
    if $target -> exists
      $arr = create new array, arguments=$target, null, null, null, null
      return $arr
    end
  else if $a.event == [MEvent.Finished]
    set global variable: name='menu.target' value=null
  end
  
  return null
  
  
  sub.BuildMenu:
  * Create target selection
  $menu = create custom menu array
  
  $added = table alloc
  
  * Add playership
  $text = sprintf: pageid=$page.id textid=219, [PLAYERSHIP], null, null, null, null
  add custom menu item to array $menu: text=$text returnvalue=[PLAYERSHIP]
  $added[[PLAYERSHIP]] = [TRUE]
  
  * add player target
  $player.target = get player tracking aim
  if $player.target -> exists
    if not $added[$player.target]
      if not $wing -> is $player.target a enemy
        $text = sprintf: pageid=$page.id textid=201, $player.target, null, null, null, null
        add custom menu item to array $menu: text=$text returnvalue=$player.target
        $added[$player.target] = [TRUE]
      end
    end
  end
  
  $player.target = [PLAYERSHIP] -> is docked
  if $player.target -> exists
    if not $added[$player.target]
      if not $wing -> is $player.target a enemy
        $text = sprintf: pageid=$page.id textid=220, $player.target, null, null, null, null
        add custom menu item to array $menu: text=$text returnvalue=$player.target
        $added[$player.target] = [TRUE]
      end
    end
  end
  
  * add the option to select target
  $text = read text: page=$page.id id=214
  add custom menu item to array $menu: text=$text nextinput='select' value=null
  
  $size = size of array $menu
  if $size == 1
    gosub sub.GetTarget
  else
    $text = read text: page=$page.id id=215
    $title = read text: page=2030 id=7
    $input = create input: id='select.menu' menu=$menu title=$title description=$text callback=null
  end
  endsub
  
  sub.GetTarget:
  $text = read text: page=$page.id id=215
  $input = create input: id='select.target', user input=Var/Ship/Station, refobj=null, description=$text, without sector=[FALSE], filterclass=null, racemask=null, flags=null, default=null
  endsub
  return null
]]>
</sourceplaintext>
