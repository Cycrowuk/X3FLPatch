<script>
<name>!move.freighttransfer</name>
<version>4</version>
<engineversion>61</engineversion>
<description>Ship Command: Freight transfer</description>
<arguments>
<argument index="1" name="a.target" type="Var/Ship/Station" desc="Target Ship" />
<argument index="2" name="a.fromWares" type="Value" desc="Wares to send" />
<argument index="3" name="a.toWares" type="Value" desc="Wares to receive" />
<argument index="4" name="a.unload" type="Var/Boolean" desc="Unload?" />
</arguments>
<sourceplaintext>
<![CDATA[
  * Advanced freight transfer command
  * uses drones to deleiver your cargo
  * ==========================================
  * Author: Cycrow
  * Updated: 2020-06-20
  * ==========================================
  skip if $a.target -> exists
    return null
  
  * load all wares into availble drones
  $aDrones = array alloc: size=0
  
  if is datatype[ $a.fromWares ] == DATATYPE_TABLE
    $tFromWares = clone table $a.fromWares
  else if is datatype[ $a.fromWares ] == DATATYPE_ARRAY
    $count = size of array $a.fromWares
    while $count
      dec $count = 
      $amt = $a.fromWares[$count]
      dec $count = 
      $ware = $a.fromWares[$count]
      $tFromWares[$ware] = $amt
    end
  else
    $tFromWares = table alloc
  end
  
  $tToWares = table alloc
  
  if is datatype[ $a.toWares ] == DATATYPE_TABLE
    $tToWares = clone table $a.toWares
  else if is datatype[ $a.toWares ] == DATATYPE_ARRAY
    $count = size of array $a.toWares
    while $count
      dec $count = 
      $amt = $a.toWares[$count]
      dec $count = 
      $ware = $a.toWares[$count]
      $tToWares[$ware] = $amt
    end
  end
  
@ $array.drones.to = [THIS] -> call script !lib.trade.splitdrones :  Ware Table=$tToWares  Drone Ship Type=Freight Drone
@ $array.drones.from = [THIS] -> call script !lib.trade.splitdrones :  Ware Table=$tFromWares  Drone Ship Type=Freight Drone
  
  $table.drones = table alloc
  
  while [TRUE]
    if $array.drones.from
      skip if size of array $array.drones.from
        $array.drones.from = null
    end
    if $array.drones.to
      skip if size of array $array.drones.to
        $array.drones.to = null
    end
    
    if ! $array.drones.from AND ! $array.drones.to
      break
    end
    
    if not [THIS] -> get true amount of ware Freight Drone in cargo bay
      gosub sub.CheckDroneTable
      if not size of table $table.drones
        break
      end
@     = wait randomly from 10000 to 30000 ms
      continue
    end
    
    $table.to = null
    $table.from = null
    
    if $array.drones.from
      $table.from = $array.drones.from[0]
      remove element from array $array.drones.from at index 0
    end
    if $array.drones.to
      $table.to = $array.drones.to[0]
      remove element from array $array.drones.to at index 0
    end
    
    if ! $table.to AND ! $table.from
      break
    end
    
  * create the freight drone
    $created.drone = [THIS] ->eject ware: ware=Freight Drone
    $created.drone ->set homebase to [THIS]
    $created.drone ->set notify command none: notify=[FALSE]
    $created.drone ->set is being managed: on=[TRUE]
    
    do if $table.from
@     = $created.drone -> call script !lib.freight.fetch :  Fetch target=[THIS]  Ware Table=$table.from  Unload?=$a.unload
    
  * move drone to target
    $task = [THIS] -> get next available task: starting=1000
    [THIS] ->start task $task with script !lib.freight.dropandreturn and prio 10: arg1=$a.target arg2=$table.to arg3=$created.drone arg4=$a.unload arg5=null
    $table.drones[$created.drone] = $task
    
@   = wait 2000 ms
  end
  
  * wait for drones to finish
  while size of table $table.drones
@   = wait 500 ms
    gosub sub.CheckDroneTable
  end
  
  return null
  
  * get the size that will fit
  sub.GetMax:
  $s.free = $created.drone -> get free amount of ware $ware in cargo bay
  if $s.free < $amt
    $amt = $s.free
  end
  endsub
  
  sub.CheckDroneTable:
  $next.drone = get next key: table=$table.drones key=null
  while $next.drone
    $check.drone = $next.drone
    $next.drone = get next key: table=$table.drones key=$next.drone
    if not $check.drone -> exists
      remove key $check.drone from table $table.drones
      continue
    end
    $task = $table.drones[$check.drone]
    if not [THIS] -> is script !lib.freight.dropandreturn on stack of task=$task
      remove key $check.drone from table $table.drones
      continue
    end
  end
  endsub
  return null
]]>
