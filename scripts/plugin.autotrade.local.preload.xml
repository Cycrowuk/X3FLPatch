<script>
<name>plugin.autotrade.local.preload</name>
<version>4</version>
<engineversion>61</engineversion>
<description>ST LT: Argument preload</description>
<arguments>
<argument index="1" name="a.obj" type="Value" desc="Ship" />
<argument index="2" name="a.state" type="Var/String" desc="State" />
<argument index="3" name="a.value" type="Value" desc="Value" />
</arguments>
<sourceplaintext>
<![CDATA[
  $pageid = 6020
  
  if $a.state == 'start'
  * ST, LT: Sector argument
    $title = read text: page=$pageid id=212
    $input = create input: id='sector', user input=Var/Sector, refobj=$a.obj, description=$title, without sector=[FALSE], filterclass=null, racemask=null, flags=null, default=null
    return $input
    
  else if $a.state == 'sector'
    $a.obj ->set local variable: name='menu.sector' value=$a.value
    gosub sub.MaxRange
    if $max.jumps > 0
      $title = sprintf: pageid=$pageid textid=302, $max.jumps, null, null, null, null
      $menu = create custom menu array
      set menu option: $menu, icon=26
      set menu option: $menu, maximum menu height=7 lines
      $text = read text: page=$pageid id=2030
      add trade bar to menu: $menu, text=$text, min=0, max=$max.jumps, current=0, return value='range', left text=null, right text='%CURRENT%'
      $size = size of array $menu
      dec $size = 
      $data = $menu[$size]
      $rank = $a.obj ->get trade skill rank
      $data = add menu trade bar text: $data id=[TradeBarText.NotoLeft] text=$rank
      $menu[$size] = $data
      $text = read text: page=$pageid id=2029
      $input = create input: id='range' menu=$menu title=$text description=$title callback=[FALSE]
      return $input
    end
    return [TRUE]
    
  else if $a.state == 'range'
    $jumps = $a.value[1]
    $a.obj ->set local variable: name='menu.range' value=$jumps
    return [TRUE]
    
  else if $a.state == 'arguments'
    $sector = $a.obj -> get local variable: name='menu.sector'
    $jumps = $a.obj -> get local variable: name='menu.range'
    skip if $jumps
      $jumps = 0
    $ret = create new array, arguments=$sector, $jumps, null, null, null
    gosub sub.ClearVars
    return $ret
    
  else if $a.state == 'closed'
    gosub sub.ClearVars
  else if $a.state == 'finished'
    gosub sub.ClearVars
  else if $a.state == 'cancelled'
    if $a.value == 'range'
      $a.obj ->set local variable: name='menu.sector' value=null
@     $input = [THIS] -> call script plugin.autotrade.local.preload :  Ship=$a.obj  State='start'  Value=null
      return $input
    end
    gosub sub.ClearVars
  end
  
  return null
  
  
  sub.ClearVars:
  $a.obj ->set local variable: name='menu.sector' value=null
  $a.obj ->set local variable: name='menu.range' value=null
  endsub
  
  sub.MaxRange:
  * LT: Jumprange argument
  $pilot = $a.obj ->get pilot speaker
  if not $pilot -> exists
    $max.jumps = 0
  else
    $experience = $pilot -> get pilot tradeskill
    if not $experience
      $max.jumps = 0
    else if $experience < 6
      $max.jumps = 0
    else if $experience < 8
      $max.jumps = 1
    else if $experience < 10
      $max.jumps = 2
    else if $experience < 12
      $max.jumps = 3
    else if $experience < 16
      $max.jumps = 4
    else if $experience < 20
      $max.jumps = 8
    else
      $max.jumps = 10
    end
  end
  endsub
  return null
]]>
</sourceplaintext>
