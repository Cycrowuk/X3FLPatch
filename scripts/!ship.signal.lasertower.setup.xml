<script>
<name>!ship.signal.lasertower.setup</name>
<version>2</version>
<engineversion>72</engineversion>
<description>Signal script to delay the activation of Lasertowers</description>
<arguments>
<argument index="1" name="Sector" type="Var/Sector" desc="sector" />
<argument index="2" name="Mode" type="Var/String" desc="mode" />
</arguments>
<sourceplaintext>
<![CDATA[
  skip if [THIS] -> exists
    return null
  * Covers both LT and terran OL:
  skip if [THIS] -> is of class Lasertower
    return null
  do if [THIS] -> get local variable: name='GZ.LT.SETUP'
    return null
  $Index.Bay = [THIS] -> get max. number of lasers in turret 0
  skip if $Index.Bay
    return null
  
  $Setup.Delay.in.sec = ( $Index.Bay * 7 + 13 ) * 30
  $Use.Task.ID = 34
  
  do if $Mode == 'start.task'
    goto label Setup.Task
  
  do if [THIS] -> is script !ship.signal.lasertower.setup on stack of task=$Use.Task.ID
    return null
  [THIS] ->start task $Use.Task.ID with script !ship.signal.lasertower.setup and prio 0: arg1=null arg2='start.task' arg3=null arg4=null arg5=null
  return null
  
  * _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _
  Setup.Task:
  * Record standard loadout
  [THIS] ->set local variable: name='GZ.LT.SETUP' value=[TRUE]
  gosub Equip.LT
  
  $Index.Bay = [THIS] -> get max. number of lasers in turret 0
  $Loadout.arr = array alloc: size=$Index.Bay
  while $Index.Bay
    dec $Index.Bay = 
    $Laser = [THIS] -> get laser type in turret 0 at slot $Index.Bay
    $Loadout.arr[$Index.Bay] = $Laser
  *[THIS] ->switch laser in turret 0 gun $Index.Bay to null
  end
  [THIS] ->set current shield strength to 1
  [THIS] ->set guns disabled: disabled=[TRUE]
  
  * Waaaait
  $Time = playing time
  $Time.End = $Time + $Setup.Delay.in.sec
  while $Time < $Time.End
    
  * In case something renames the satellite, use the new name
    $Name = [THIS] -> get name
    if $Name != $Old.Name
      $Old.Name = $Name
      $Original.Name = $Name
    end
    
    $Time = playing time
    $Time.to.go = $Time.End - $Time
    $Time.to.go = get maximum, $Time.to.go, 0, null, null, null
    $Name = format time: $Time.to.go
    $Name = get substring of $Name offset=3 length=5
    $Name = sprintf: fmt='%s  (%s)', $Original.Name, $Name, null, null, null
    [THIS] ->set name to $Name
    $Old.Name = $Name
    
    if [THIS] -> is in active sector
@     = wait randomly from 1500 to 2500 ms
    else
@     = wait randomly from 9000 to 14000 ms
    end
  end
  [THIS] ->set name to $Original.Name
  
  * The shield regen task does not start when deployed OOS.
  skip if [THIS] -> is in active sector
    [THIS] ->set current shield strength to 2000000000
  
  * Restore standard loadout
  $Index.Bay = size of array $Loadout.arr
  while $Index.Bay
    dec $Index.Bay = 
    $Laser = $Loadout.arr[$Index.Bay]
    [THIS] ->switch laser in turret 0 gun $Index.Bay to $Laser
  end
  [THIS] ->set guns disabled: disabled=[FALSE]
  return null
  
  * _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _
  Equip.LT:
  $Shield.Type = [THIS] -> get max. shield type that can be installed
  if $Shield.Type
    if not [THIS] -> get amount of ware $Shield.Type in cargo bay
      $Index.Bay = [THIS] -> get number of shield bays
      do if $Shield.Type AND $Index.Bay
        = [THIS] -> install $Index.Bay units of $Shield.Type
    end
  end
  
  $Laser.arr = [THIS] -> get compatible laser array: turret=0
  if is datatype[ $Laser.arr ] == DATATYPE_ARRAY
    $Index.Bay = size of array $Laser.arr
    while $Index.Bay AND ! $Laser
      dec $Index.Bay = 
      $Laser = $Laser.arr[$Index.Bay]
      skip if [THIS] -> can transport ware $Laser
        $Laser = null
      do if $Laser == Repair Laser OR $Laser == Tractor Beam OR $Laser == Mobile Drilling System
        $Laser = null
    end
    
    if $Laser
      if not [THIS] -> get amount of ware $Laser in cargo bay
        $Index.Bay = [THIS] -> get max. number of lasers in turret 0
        if $Index.Bay
          = [THIS] -> install $Index.Bay units of $Laser
          $Ammo = get ammunition of laser $Laser
          do if $Ammo
            = [THIS] -> add $Index.Bay units of $Ammo
        end
      end
    end
  end
  endsub
  return null
]]>
</sourceplaintext>
