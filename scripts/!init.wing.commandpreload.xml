  set wing command preload script: wing command=COMMAND_WING_SUPPLY script=!wing.cmd.supply.pl
  
  set wing command menu script: wing command=COMMAND_WING_MOVE_SECTOR, script=!wing.cmd.movesector.menu, cmdcheck=[FALSE]
  set wing command menu script: wing command=COMMAND_WING_DOCKAT, script=!wing.cmd.movestation.menu, cmdcheck=[FALSE]
  set wing command menu script: wing command=COMMAND_WING_FOLLOW, script=!wing.cmd.follow.menu, cmdcheck=[FALSE]
  set wing command menu script: wing command=COMMAND_WING_JUMP, script=!wing.cmd.jump.menu, cmdcheck=[FALSE]
  set wing command menu script: wing command=COMMAND_WING_PROTECT, script=!wing.cmd.protect.pl, cmdcheck=[FALSE]
  return null
]]>
