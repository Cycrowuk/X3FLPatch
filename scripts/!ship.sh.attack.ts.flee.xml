<script>
<name>!ship.sh.attack.ts.flee</name>
<version>2</version>
<engineversion>62</engineversion>
<description>called from signal attacked on TS ships</description>
<arguments>
<argument index="1" name="attacker" type="Var/Ship/Station" desc="the attacker" />
<argument index="2" name="station" type="Var/Station" desc="station to dock at" />
</arguments>
<sourceplaintext>
<![CDATA[
  do if [THIS] ->is from mission
    write to player logbook: printf: type=[Log.Debug], fmt='%s: Fleeing, %s', [THIS], $station, null, null, null
  
  do if [THIS] -> is of class Stationary Ship
    return [FALSE]
  
  if $station
    $override = [THIS] ->get command display override
    $read.text = read text: page=6020 id=2160
    [THIS] ->set command display override: text=$read.text
    $previousdest = [THIS] -> get destination
    
@   = [THIS] -> call script plugin.turbo.npcuse : 
    
    [THIS] ->set destination to $station
    do if [THIS] ->is from mission
      write to player logbook: printf: type=[Log.Debug], fmt='%s: moving to station', [THIS], $station, null, null, null
@   $ret = [THIS] -> call script !move.movetostation :  station=$station
    do if [THIS] ->is from mission
      write to player logbook: printf: type=[Log.Debug], fmt='%s: at station?', [THIS], $station, null, null, null
    
    do if $previousdest -> exists
      [THIS] ->set destination to $previousdest
    
    if [THIS] -> is docked
      $read.text = read text: page=6020 id=2161
      [THIS] ->set command display override: text=$read.text
@     = wait randomly from 60000 to 300000 ms
      [THIS] ->set command display override: text=$override
    end
    [THIS] ->set command display override: text=$override
    return [TRUE]
  end
  
  return [FALSE]
]]>
</sourceplaintext>
